<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cooldudes.stalkmarket">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">



        <activity android:name="cooldudes.stalkmarket.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Onboarding1Activity" />
        <activity android:name=".Onboarding2Activity" />
        <activity android:name=".Onboarding3Activity" />
        <activity android:name=".Onboarding4Activity" />
        <activity android:name=".DiversifyActivity" />
        <activity android:name=".SupplyActivity" />
        <activity android:name=".CompoundActivity" />

        <activity android:name=".MainActivity" />
        <activity android:name=".FamilyMemberProfile" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <receiver
            android:name=".AlarmReceiver"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
            </intent-filter>
        </receiver>
    </application>

</manifest>